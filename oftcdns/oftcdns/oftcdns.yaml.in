# oftcdns configuration file
dns:
  interface: "127.0.0.1"
  port: 20053
  default region: "global"
  region database: "@LIBDIR@/@PACKAGE@/ip2region.dat"
  zone: "geo.oftc.net"
  contact: "hostmaster.oftc.net"
  serial: 1
  refresh: 600
  minimum: 600
  expire: 600
  retry: 600
  ttl: 60
  name servers:
    - "gns1.oftc.net"
    - "gns2.oftc.net"
    - "gns3.oftc.net"
    - "gns4.oftc.net"
  regions:
    eu:
      - "1.1.1.1" # eu1
      - "1.1.1.2" # eu2
      - "1.1.1.3" # eu3
    na:
      - "1.1.2.1" # na1
      - "1.1.2.2" # na2
      - "1.1.2.3" # na3
    oc:
      - "1.1.3.1" # oc1
      - "1.1.3.2" # oc2
      - "1.1.3.3" # oc3
    uq:
      - "1.1.4.1" # uq1
      - "1.1.4.2" # uq2
      - "1.1.4.3" # uq3
    global:
      - "1.1.1.1" # eu1
      - "1.1.1.2" # eu2
      - "1.1.1.3" # eu3
      - "1.1.2.1" # na1
      - "1.1.2.2" # na2
      - "1.1.2.3" # na3
irc:
  server: "irc.oftc.net"
  port: 6667
  nickname: "oftcdns"
  channel: "blah"
